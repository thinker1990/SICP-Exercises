#lang sicp

(define (a-pythagorean-triple-between low high)
  (let ((i (an-integer-between low high))
        (hsq (* high high)))
    (let ((j (an-integer-between i high)))
      (let ((ksq (+ (* i i) (* j j))))
        (require (>= hsq ksq))
        (let ((k (sqrt ksq)))
          (require (integer? k))
          (list i j k))))))

;; yes, Ben's solution is more efficient.
;; for (require (>= hsq ksq)) eliminate some combinations of i and j would be tested in the original version. 
